    "chai": "^4.3.6",
    "eslint": "^8.24.0",
    "husky": "^8.0.1",
    "openapi-forge": "latest",
    "prettier": "^2.7.1",
    "semantic-release": "^19.0.5"
  },
  "engines": {
    "node": ">=16.0.0"
  }
